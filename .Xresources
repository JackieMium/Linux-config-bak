Xcursor.theme: macOSMonterey
Xcursor.size: 22

! XTERM and UXterm

XTerm*faceName:  Fantasque Sans Mono:style=Regular
! support chinese
XTerm*faceNameDoublesize: Droid Sans Fallback:style=Regular
XTerm*faceSize: 12

XTerm*vt100.geometry: 160x45
XTerm*loginshell: true
! support 256color
XTerm*termName: xterm-256color
! set Ctrl + V for paste   http://forum.ubuntu.org.cn/viewtopic.php?t=143221
XTerm*VT100.Translations: #override \
    Ctrl Shift <Key> V: insert-selection(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
    <BtnUp>: select-end(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
    Ctrl <KeyPress> P: print() \n
XTerm*visualBell: true
XTerm*audioBell: false

XTerm*cursorBlink: true

XTerm*locale: true
XTerm.utf8:     true
XTerm*utf8Title: true

UXTerm*visualBell: true
UXTerm*audioBell: false
UXTerm*faceName: Fantasque Sans Mono:style=Regular
! support chinese
UXTerm*faceNameDoublesize: Droid Sans Fallback:style=Regular
UXTerm*faceSize: 12

UXTerm*vt100.geometry: 160x45
UXTerm*loginshell: true
! support 256color
UXTerm*termName: xterm-256color
! set Ctrl + V for paste   http://forum.ubuntu.org.cn/viewtopic.php?t=143221
UXTerm*VT100.Translations: #override \
    Ctrl Shift <Key> V: insert-selection(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
    <BtnUp>: select-end(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
    Ctrl <KeyPress> P: print() \n


